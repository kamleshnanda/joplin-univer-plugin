{
  "name": "joplin-plugin-univer",
  "version": "0.1.0",
  "description": "Excel-like spreadsheet plugin for Joplin using Univer SDK",
  "author": "Kamlesh Nanda <kamleshnanda@github>",
  "license": "MIT",
  "keywords": [
    "joplin",
    "plugin",
    "spreadsheet",
    "excel",
    "univer",
    "markdown"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kamleshnanda/joplin-univer-plugin.git"
  },
  "scripts": {
    "dist": "webpack --mode production",
    "build": "webpack --mode development",
    "dev": "webpack --mode development --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "package": "npm run dist && cd dist && zip -r ../joplin-plugin-univer.jpl . && cd .."
  },
  "engines": {
    "node": ">=16"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.0",
    "copy-webpack-plugin": "^12.0.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.90.0",
    "webpack-cli": "^5.1.4",
    "fast-check": "^3.15.1",
    "@joplin/lib": "^2.14.0"
  },
  "dependencies": {
    "@univerjs/core": "^0.1.0",
    "@univerjs/design": "^0.1.0",
    "@univerjs/docs": "^0.1.0",
    "@univerjs/docs-ui": "^0.1.0",
    "@univerjs/engine-formula": "^0.1.0",
    "@univerjs/engine-render": "^0.1.0",
    "@univerjs/sheets": "^0.1.0",
    "@univerjs/sheets-formula": "^0.1.0",
    "@univerjs/sheets-ui": "^0.1.0",
    "@univerjs/ui": "^0.1.0"
  }
